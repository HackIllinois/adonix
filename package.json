{
    "name": "adonix",
    "version": "1.0.0",
    "description": "Official repository for API redesign.",
    "main": "api/index.js",
    "type": "module",
    "scripts": {
        "devdocs": "npx typedoc --options typedoc.json --plugin typedoc-plugin-missing-exports",
        "apidocs": "apidoc -i . -o apidocs/",
        "docs": "npm run devdocs && npm run apidocs",
        "clean-docs": "rm -r apidocs/ devdocs/",
        "gen-jwt": "node scripts/generate-jwt-token",
        "format:check": "yarn run prettier --check '**/*.{ts,js,cjs,json}'",
        "format": "yarn run prettier --write '**/*.{ts,js,cjs,json}'",
        "lint:check": "yarn run eslint src --ext .js,.jsx,.ts,.tsx",
        "lint": "yarn run eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "verify": "yarn build && yarn lint:check && yarn format:check",
        "serve": "node scripts/serve.mjs",
        "start": "vercel dev"
    },
    "keywords": [],
    "author": "Aydan Pirani <aydanpirani@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "@typegoose/typegoose": "^11.5.0",
        "@types/uuid": "^9.0.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.1",
        "mongodb": "^5.7.0",
        "mongoose": "^7.5.3",
        "morgan": "^1.10.0",
        "ms": "^2.1.3",
        "passport": "^0.6.0",
        "passport-github": "^1.1.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-strategy": "^1.0.0",
        "uuid": "^9.0.1"
    },
    "repository": "git@github.com:HackIllinois/adonix.git",
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-serve-static-core": "^4.17.35",
        "@types/express-session": "^1.17.7",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/ms": "^0.7.31",
        "@types/node": "^20.4.8",
        "@types/passport": "^1.0.12",
        "@types/passport-github": "^1.1.7",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/passport-strategy": "^0.2.35",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "apidoc": "^1.2.0",
        "eslint": "^8.46.0",
        "inquirer": "^9.2.11",
        "open": "^9.1.0",
        "prettier": "3.0.3",
        "typedoc": "^0.24.8",
        "typedoc-plugin-missing-exports": "^2.0.1",
        "typescript": "^5.1.6",
        "vercel": "^32.4.1"
    }
}
