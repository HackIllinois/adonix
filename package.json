        "mongoose": "^7.5.3",
        "morgan": "^1.10.0",
        "ms": "^2.1.3",
        "passport": "^0.6.0",
        "passport-github": "^1.1.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-strategy": "^1.0.0",
        "status-code-enum": "^1.0.0",
        "swagger-ui-express": "^5.0.1",
        "tsc-watch": "^6.2.0",
        "uuid": "^9.0.1",
        "zod": "^3.23.8"
